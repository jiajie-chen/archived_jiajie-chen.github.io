{
  "name": "Motivational Phrases",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Adds motivational phrases to articles",
  "homepage_url": "http://www.jiajie-chen.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "lib/tracery.js","lib/jquery-1.11.1.min.js","inject.js"
      ]
    }
  ]
}